<p id="notice"><%= notice %></p>

<h1>Lends</h1>

<table>
  <thead>
    <tr>
      <th>Value</th>
      <th>Loan date</th>
      <th>Deliver date</th>
      <th>Movie</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lends.each do |lend| %>
      <tr>
        <td><%= lend.value %></td>
        <td><%= lend.loan_date %></td>
        <td><%= lend.deliver_date %></td>
        <td><%= lend.movie %></td>
        <td><%= lend.user %></td>
        <td><%= link_to 'Show', lend %></td>
        <td><%= link_to 'Edit', edit_lend_path(lend) %></td>
        <td><%= link_to 'Destroy', lend, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lend', new_lend_path %>
